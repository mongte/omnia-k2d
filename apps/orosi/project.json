{
  "name": "orosi",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/orosi/lib",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "apps/orosi"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "apps/orosi"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/orosi"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/orosi"
      }
    }
  },
  "tags": []
}
